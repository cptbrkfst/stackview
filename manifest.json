{
  "manifest_version": 2,
  "name": "Stack View",
  "version": "0.557",
  "background": {
    "scripts": ["./background.js"],
    "persist": true
  },
  "icons": {
    "16": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["./jquery-3.2.1.min.js", "./content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Stack View"
  }
}
